program example4;
var a, b : integer;
begin
        readln(a, b);
        if a > b then
        begin
                writeln(a, " is greater than ", b);
        end;
        if a = b then
        begin
                writeln(a, " is equal to ", b);
        end;
        if a < b then
        begin
                writeln(a, " is greater then ", b);
        end;
end.